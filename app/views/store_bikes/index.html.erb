
<h1 class="title">Bicycle Listing</h1>

<table>
  <thead>
 
      <tr class="col-md-0 col-md-offset-6">
      <td>Store</td>
      <td>Bicycle</td>
      <td>Price</td>
      <!--th colspan="3"></th-->
    </tr>
  </thead>
  <% if signed_in? && (current_user.admin?) %>
    <%= link_to 'New Store Bike', new_store_bike_path %>
  <% end %>
  <tbody>
    
    
    <% @store_bikes.each do |store_bike| %>
    <tr >
     
   
        <td><%= store_bike.store.name %></td>
     
        <td><%= store_bike.bike.name %> </td>

        <td><%= store_bike.bike.price %> </td>
        <td><%= link_to "Show all bookings", store_store_bike_bookings_path(@store, store_bike) %></td>
        <td><%= link_to "Add booking", new_store_store_bike_booking_path(@store, store_bike) %></td>

    </tr>
    <% end %>
        
  </tbody>
</table>
